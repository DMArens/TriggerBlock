{
	"manifest_version": 2,

		"name": "Trigger Block",
		"description": "Block those pesky trigger images",
		"version": "0.1.0",

		"browser_action": {
		  "default_title": "Trigger Block"
		},
		"options_page": "options.html",
		"permissions": [
			"storage",
			"<all_urls>",
			"tabs",
			"contextMenus"
		],
		"icons": {
		},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["/src/lib/clarifai-2.0.9.js", "/src/lib/jquery-3.1.1.min.js", "/src/main.js"],
			"css": ["/styles/overlay.css"]
		}
	]

}
